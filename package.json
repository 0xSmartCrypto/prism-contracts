{
  "name": "prism-test",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@terra-money/terra.js": "^2.0.0-beta.15",
    "moment": "^2.29.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "eslint": "^7.31.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-unicorn": "^34.0.1",
    "xo": "^0.39.1"
  },
  "xo": {
    "extends": "airbnb-base",
    "rules": {
      "spaced-comment": 0,
      "semi": [
        "error",
        "never"
      ],
      "no-mixed-spaces-and-tabs": 1,
      "no-tabs": 1,
      "indent": [
        "error",
        4
      ],
      "no-underscore-dangle": 0,
      "comma-dangle": 0,
      "import/order": [
        0
      ],
      "class-methods-use-this": 0,
      "arrow-parens": 0,
      "no-use-before-define": [
        "error",
        {
          "functions": false,
          "classes": false,
          "variables": true
        }
      ],
      "quotes": 0,
      "operator-linebreak": [
        "error",
        "after"
      ],
      "camelcase": 0,
      "no-plusplus": [
        "error",
        {
          "allowForLoopAfterthoughts": true
        }
      ],
      "promise/prefer-await-to-then": 0,
      "unicorn/catch-error-name": [
        "error",
        {
          "name": "err"
        }
      ],
      "linebreak-style": [
        "error",
        "unix"
      ],
      "unicorn/prevent-abbreviations": 0,
      "max-len": 0,
      "no-restricted-syntax": 0,
      "unicorn/prefer-optional-catch-binding": 0,
      "no-await-in-loop": 0,
      "import/prefer-default-export": 0,
      "import/extensions": 0,
      "unicorn/prefer-module": 0
    }
  }
}
